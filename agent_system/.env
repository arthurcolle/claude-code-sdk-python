# Agent System Configuration
AGENT_SYSTEM_NAME=ComplexAgentSystem
AGENT_SYSTEM_PORT=8888

# Tool Registry Configuration (disabled for local development)
TOOL_REGISTRY_URL=http://localhost:2016
TOOL_REGISTRY_TOKEN=

# Database Configuration (using defaults for local development)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# Redis Configuration (using defaults for local development)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Agent Communication
MESSAGE_QUEUE_PREFIX=agent_system
AGENT_HEARTBEAT_INTERVAL=30

# WebSocket Configuration
ENABLE_WEBSOCKET=true
WS_HEARTBEAT_INTERVAL=30

# Agent Limits
MAX_AGENTS_PER_ROLE=5
MAX_CONCURRENT_TASKS=10
TASK_TIMEOUT=300

# Tool Management
AUTO_VALIDATE_TOOLS=false
TOOL_VALIDATION_THRESHOLD=0.8
DISABLE_TOOL_REGISTRY=true

# Logging
LOG_LEVEL=INFO
ENABLE_TRACE_LOGGING=false